		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/custom_game/watch_tower_channelbar.vcss_c" />
	</styles>
	<DOTAWatchTowerChannelBar hittest="false" hittestchildren="false">
		<Label class="Normal TitleText" text="#DOTA_Hud_Outpost_ChannelBar_Title" />
		<Label class="Elite TitleText" text="#DOTA_Hud_Outpost_ChannelBar_EliteTitle" html="true" />
		<Label class="Trap TitleText" text="#DOTA_Hud_Outpost_ChannelBar_TrapTitle" html="true" />
		<Label class="Boss TitleText" text="#DOTA_Hud_Outpost_ChannelBar_BossTitle" html="true" />
		<Label class="Bonus TitleText" text="#DOTA_Hud_Outpost_ChannelBar_BonusTitle" html="true" />
		<Label class="Rest TitleText" text="#DOTA_Hud_Outpost_ChannelBar_RestTitle" html="true" />
		<Label class="EncounterName" text="{s:encounter_name}" />
	</DOTAWatchTowerChannelBar>
</root>


